/* pages/myWallet/myWallet.wxss */
@import "../lib/font/BebasNeue-Regular.wxss";
.weui-flex__item{
  text-align: center;
}
.weui-flex__item .placeholder-line{
  border-right: 1px solid #e5e5e5;
}
.weui-flex__item .wallet-t1 text{
  font-size: 104rpx;
  color: #f36902;
  /* font-weight: bold; */
  font-family: 'Bebas Neue';
}
.weui-flex__item .wallet-t2{
  font-size: 16px;
}
.weui-flex{
  margin: 15px;
  padding: 15px 0;
  border-radius: 12px;
  box-shadow:0px 4px 20px #e0e0e0;
  margin-bottom: 30px;
}
.weui-btn{
  background: linear-gradient(to right, #f36902 , #ff9911);
}
.weui-btn-area-new{
  text-align: center;
  font-size: 24rpx;
  color: #9f9f9f;
  padding-top: 30rpx;
}