<!--pages/workHave/workHave.wxml-->
<view class="page">
    <view class="weui-msg">
      <view class='recharge-box'>
        <view class="weui-msg__icon-area page-section-ctn">
            <!-- <icon type="warn" size="93"></icon> -->
            <image class='phone-icon' src='../../images/xiyi.png' animation="{{animationData}}"></image>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">洗衣进行中</view>
            <!-- <view class="weui-msg__desc">放好衣物后，按洗衣机上的“开始”按钮即可开始洗衣</view> -->
        </view>
      </view>
      <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">洗衣编号</view>
                <view class="weui-cell__ft">{{systemOrderNo}}</view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__bd">洗衣机编号</view>
                <view class="weui-cell__ft">{{deviceCode}}</view>
            </view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn weui-btn_mini weui-btn_default weui-btn-1" type="primary" bindtap="showTopTipsRepair">设备报修</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            
        </view>
    </view>

    <view class="weui-msg__extra-area">
        <view class="weui-footer">
            <view class="weui-footer__links">
                <view  class="weui-footer__link">有其他疑问？请<text class="customer-service" type="primary" bindtap="customerService">联系客服</text></view>
            </view>
            <!-- <view class="weui-footer__text">Copyright © 2008-2016 weui.io</view> -->
        </view>
    </view>
</view>