/* pages/aboutUs/aboutUs.wxss */
.about-box{
  background-color: #f5f5f5;
  padding: 25px 0;
  margin-bottom: 15px;
}
.weui-msg{
  padding-top: 0;
}
.weui-msg__icon-area {
  width: 208rpx; /*104px*/
  text-align: center;
  margin: 0 auto;
}
.weui-msg__title{
  color: #000;
}
.weui-msg__text-area{
  margin-bottom: 0;
}
.weui-msg__icon-area image{
  width: 100%;
  height: 214rpx; /*107px*/
  overflow: inherit;
}
.weui-cells{
  font-size: 16px;
}
.weui-cell__bd{
  text-align: left;
}
.weui-cell__ft {
  color:#000;
}
.weui-cells:before,.weui-cells:after,.weui-cell:before{
  display: none;
}
.weui-msg__text-area .weui-msg__title-name{
  color: #959595;
  font-size:18px;
}
.weui-msg__text-area .weui-msg__title-number{
  border-radius: 20rpx;
  background-color: #e6e6e6;
  color: #ffffff;
  font-size:28rpx;
  padding: 2rpx 30rpx;
  display: inline-block
}
.weui-cell__ft-tel{
  color: #fe930f;
}