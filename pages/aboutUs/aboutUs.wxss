/* pages/aboutUs/aboutUs.wxss */
.about-box{
  background-color: #f5f5f5;
  padding: 25px 0;
  margin-bottom: 15px;
}
.weui-msg{
  padding-top: 0;
}
.weui-msg__icon-area {
  width: 40%;
  text-align: center;
  margin: 0 auto;
}
.weui-msg__title{
  color: #000;
}
.weui-msg__text-area{
  margin-bottom: 0;
}
.weui-msg__icon-area image{
  width: 100%;
  height: 290rpx;
  overflow: inherit;
}
.weui-cells{
  font-size: 16px;
}
.weui-cell__bd{
  text-align: left;
}
.weui-cell__ft {
  color:#000;
}
.weui-cells:before,.weui-cells:after,.weui-cell:before{
  display: none;
}